<header class='header'>
  <a class='header-link' href='https://www.blippit.com/' target='_blank'>
    <img class='header-logo' src='{{path_to_root}}img/logo.png'/>
  </a>

  <p class='header-col header-col--primary'>
    {{#doc_coverage}} ({{doc_coverage}}% documented){{/doc_coverage}}
  </p>

  {{#github_url}}
    <p class='header-col header-col--secondary'>
      <a class='header-link' href='{{github_url}}' target='_blank'>
        <img class='header-icon' src='{{path_to_root}}img/gl.png'/>
        View on GitLab
      </a>
    </p>
  {{/github_url}}

  {{#dash_url}}
    <p class='header-col header-col--secondary'>
      <a class='header-link' href='{{dash_url}}' target='_blank'>
        <img class='header-icon' src='{{path_to_root}}img/dash.png'/>
        Install in Dash
      </a>
    </p>
  {{/dash_url}}
</header>
